package basicprogramsdayone;

public class InitialsInLetters
{
	public static void main(String[] args)
	{
	//	System.out.println("12345   12345");
		System.out.println(" SSS    RRRR ");
		System.out.println("S    S  R   R");
		System.out.println("S       R   R");
		System.out.println(" SSS    RRRR ");
		System.out.println("    S   R R  ");
		System.out.println("S   S   R  R ");
		System.out.println(" SSS    R   R");
		int m1=10,m2=20;
		System.out.println("sum is :"+m1+m2);
	}
}
