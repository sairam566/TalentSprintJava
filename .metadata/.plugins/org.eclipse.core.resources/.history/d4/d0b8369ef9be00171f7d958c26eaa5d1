package basicprograms;

public class InitialsInLetters
{
	public static void main(String[] args)
	{
		System.out.println(" SSS ");
		System.out.println("S    S");
		System.out.println("S");
		System.out.println(" SSS ");
		System.out.println("    S");
		System.out.println("S   S");
		System.out.println(" SSS");
	}
}
